{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/socket.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/socket.io/(.*)",
      "dest": "/api/socket.js"
    },
    {
      "src": "/api/socket",
      "dest": "/api/socket.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

